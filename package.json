{
  "name": "binance-pay-merchant-api",
  "version": "1.0.0",
  "description": "Binance Pay Merchant API wrapper with TypeScript",
  "author": "Thammarong Glomjai <thammrong@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": false,
  "keywords": [
    "binance",
    "binance-pay",
    "merchant",
    "api",
    "wrapper",
    "typescript"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --declaration",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "tsc -w --declaration",
    "start:debug": "tsc --debug --watch --declaration",
    "start:prod": "node dist/index"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
